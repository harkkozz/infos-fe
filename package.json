{
  "name": "infos-fe",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite --host",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src",
    "lint": "eslint src --fix --cache",
    "lint:tsc": "tsc --project tsconfig.json",
    "lint:staged": "lint-staged",
    "lint:eslint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "ts-coverage": "type-coverage",
    "codegen:watch": "graphql-codegen --watch",
    "codegen:generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@iconscout/react-unicons": "^2.0.2",
    "antd": "^5.7.0",
    "classnames": "^2.3.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "graphql": "^16.7.1",
    "i18next": "^23.2.11",
    "i18next-browser-languagedetector": "^7.1.0",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-jwt": "^1.2.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-select": "^5.7.4",
    "react-toastify": "^9.1.3",
    "sass": "^1.63.6",
    "scss": "^0.2.4",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.0",
    "c8": "^9.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "happy-dom": "^11.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "type-coverage": "^2.26.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "redux",
        "rxjs"
      ]
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write",
    "src/**/*.{j,t}s?(x)": "eslint --fix --cache"
  }
}
